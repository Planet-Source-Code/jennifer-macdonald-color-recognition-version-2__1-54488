Private Sub ScrollBlue_Change()
    'change the caption and the back color of the picture
    Me.lblblue = Me.ScrollBlue
    Call ChangeBackColor(Me.ScrollRed, Me.ScrollGreen, Me.ScrollBlue)
    Me.Label4.Caption = "The color is " & WhatColor
End Sub

Private Sub ScrollGreen_Change()
    'change the caption and the back color of the picture
    Me.lblGreen = Me.ScrollGreen
    Call ChangeBackColor(Me.ScrollRed, Me.ScrollGreen, Me.ScrollBlue)
    Me.Label4.Caption = "The color is " & WhatColor
End Sub

Private Sub ScrollRed_Change()
    'change the caption and the back color of the picture
    Me.lblRed = Me.ScrollRed
    Call ChangeBackColor(Me.ScrollRed, Me.ScrollGreen, Me.ScrollBlue)
    Me.Label4.Caption = "The color is " & WhatColor
End Sub
Private Sub ChangeBackColor(RVal As Integer, GVal As Integer, BVal As Integer)
    'change the back color of the picture to reflect the croll bar values
    RVal = Me.ScrollRed
    GVal = Me.ScrollGreen
    BVal = Me.ScrollBlue
    Call WhatColor
    Me.Picture1.BackColor = RGB(RVal, GVal, BVal)
End Sub

Public Function Brightness(mycolor As Integer) As String
    'see what the brightness of the color is: dark, medium or light
    '"D" for Dark, "L" for light and "M" for med
    'If R value and G value and B Value are all less than 85 then it is a dk color
    'so will probably be either black or a dk grey (Example (50,50,50) would be a dk grey)
    'Same goes for R,G and B value greater than 170.  The closer the 3 values get to 255, the closer the
    'color will be to white.
    If mycolor < 28.33 Then
        'ultra dark
        Brightness = "UD"
    ElseIf mycolor < 56.66 Then
        'very dark
        Brightness = "VD"
    ElseIf mycolor < 84.99 Then
        'Dark
        Brightness = "DD"
    ElseIf mycolor < 113.32 Then
        'for a lack of a better word we'll use Dark medium, Medium Medium and Light Medium
        Brightness = "DM"
    ElseIf mycolor < 141.65 Then
        Brightness = "MM"
    ElseIf mycolor < 169.98 Then
        Brightness = "LM"
    ElseIf mycolor < 198.31 Then
        'Medium Light
        Brightness = "LL"
    ElseIf mycolor < 226.64 Then
        'Very light
        Brightness = "VL"
    Else
        'ultra light
        Brightness = "UL"
    End If
End Function

Public Function HowWhite(mycolor As Integer) As String
'this function is called when R,G and B values are all above 226.65
'and will distinguish between the white, off whites etc..
'Because white is such a pue color, it doesn't take
'much to be overwhelmed and for it to take on the tint of another color
If mycolor < 236.1 Then
    HowWhite = "D"
ElseIf mycolor < 245.53 Then
    HowWhite = "M"
Else
    HowWhite = "L"
End If
End Function
